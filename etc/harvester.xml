<harvesters>
    <harvester name="GGC_def">
        <oai_baseurl>http://services.kb.nl/mdo/oai</oai_baseurl>
        <oai_setname>GGC</oai_setname>
        <oai_metadataprefix>INDEXING</oai_metadataprefix>
        <oai_resumption_token>null</oai_resumption_token>
